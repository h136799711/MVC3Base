<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    @* 引入Jquery的智能感知文件 *@
    @if (false)
    {
        <script src="@Url.Content("~/Scripts/jquery/1.9.1-vsdoc.js")" type="text/javascript"></script>
    }
    @* 引入easyui的CSS文件 *@
    <link href="@Url.Content("~/Content/jqueryui/flick/1.10.2.min.css")" rel="stylesheet" type="text/css" />
    @* 引入easyui和Jquery的JS文件 *@
    <script src="@Url.Content("~/Scripts/jquery/1.9.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery/easyui/1.3.2.min.js")" type="text/javascript"></script>
</head>

<body>
    @RenderBody()
</body>
</html>
